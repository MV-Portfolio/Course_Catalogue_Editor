%h1 Listing course_histories

%table
  %thead
    %tr
      %th
      %th
      %th

  %tbody
    - @course_histories.each do |course_history|
      %tr
        %td= link_to 'Show', course_history
        %td= link_to 'Edit', edit_course_history_path(course_history)
        %td= link_to 'Destroy', course_history, method: :delete, data: { confirm: 'Are you sure?' }

%br

= link_to 'New Course history', new_course_history_path
